<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="DisneylandMap.App"
>

    <Application.Styles>
        <FluentTheme Mode="Dark"/>

        <StyleInclude Source="/Styles/Default.axaml"></StyleInclude>

        <StyleInclude Source="/Components/AttractionNum.axaml"></StyleInclude>
        <StyleInclude Source="/Components/ItineraireInfo.axaml"></StyleInclude>
        <StyleInclude Source="/Components/NavButton.axaml"></StyleInclude>
        <StyleInclude Source="/Components/AttractionItem.axaml"></StyleInclude>
        <StyleInclude Source="/Components/GraphItem.axaml"></StyleInclude>

        
    </Application.Styles>

    <Application.Resources>

        <SolidColorBrush x:Key="LightBlue">#62CDFF</SolidColorBrush>
        <SolidColorBrush x:Key="Blue">#0A84FF</SolidColorBrush>
        <SolidColorBrush x:Key="Red">#FF2D55</SolidColorBrush>
    </Application.Resources>

</Application>